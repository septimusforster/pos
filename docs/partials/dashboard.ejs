    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <style>
            * {
                margin: 0;
                padding: 0;
                box-sizing: border-box;
            }
            #main {
                /*background-color: #eee;*/
                background-color: #445793;
                display: flex;
                font-family: "Readex Pro", sans-serif;
                font-size: 20px;
            }
            section#left, section#right {
                min-height: 100vh;
                min-height: 100dvh;
            }
            section#left {
                flex: 1;
                background-color: #fff;
                box-shadow: 3px 0 12px 5px rgba(0,0,0,0.05);
                position: relative;
            }
            section#right {
                flex: 5;
                padding: 20px 0;
                display: flex;
            }
            section > div {
                padding: 20px;
            }
            header {
                margin: 30px 0;
                text-align: center;
            }
            header svg {
                cursor: pointer;
                transition: 250ms;
            }
            header svg:hover {
                transform: scale(1.1);
            }
            footer {
                position: absolute;
                bottom: 10px;
                padding: 20px;
                width: 100%;
            }
            footer > button {
                display: flex;
                align-items: center;
                gap: 1em;
                padding: 14px 16px;
                /* background: transparent; */
            }
            button {
                outline: 0;
                border: 0;
                cursor: pointer;
                background-color: transparent;
            }
            div#sidenav {
                height: 60vh;
                height: 60dvh;
                overflow-y: auto;
            }
            div#sidenav, footer {
                display: flex;
                flex-direction: column;
            }
            div#sidenav div {
                display: flex;
                flex-direction: column;
            }
            details button, #addStudent {
                text-align: left;
                display: flex;
                align-items: center;
                justify-content: space-between;
                padding: 8px 25px;
            }
            details button:hover,
            details button:focus,
            #addStudent:hover,
            footer button:hover {
                background-color: #ccc;
            }
            details button span {
                padding: 8px 12px;
                background-color: #eee;
                border-radius: 3px;
            }
            summary, #addStudent {
                cursor: pointer;
                font-size: 0.7em;
                padding: 16px;
            }
            summary:hover, summary:focus {
                background-color: #eee;
            }
            details > details summary {
                padding-left: 26px;
            }
            h5#dashTitle {
                font-weight: 300;
                text-align: center;
                margin: 30px 0;
            }
            aside h2 {
                color: #fff;
                font-weight: normal;
            }
            aside, aside form {
                display: flex;
                flex-direction: column;
                gap: 1em;
            }
            aside {
                /* margin: 0px 4em; */
                border-radius: 5px;
                padding: 1em;
                width: 100%;
                justify-content: flex-start;
            }
            aside:hover {
                box-shadow: 0 0 3px 5px rgba(0,0,0,0.1);
            }
            aside form {
                margin-top: 30px;
            }
            aside form input, aside form button, aside form label {
                padding: 14px;
                font-size: 0.7em;
                border-radius: 5px;
            }
            aside form button {
                color: #fff;
                width: 100%;
                background-color: #283e87;
            }
            form#delArms div {
                color: #fff;
            }
            form#delArms input{
                padding: 10px;
            }
            aside#asideR form label {
                margin-left: 16px;
                cursor: pointer;
            }
            #span-settings-svg {
                position: fixed;
                top: 0;
                background-size: cover;
                width: 100%;
                height: 100%;
                background-color: rgba(0, 0, 0, 0.5);
                display: none;
                justify-content: center;
                align-items: center;
            }
            .settings-svg {
                animation: spin 3s linear infinite;
            }
            @keyframes spin {
                100% {
                    transform: rotate(360deg);
                }
            }
        </style>
        <link rel="stylesheet" type="text/css" href="/css/classroom.css">
    </head>
